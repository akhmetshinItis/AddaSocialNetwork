@model Contracts.Requests.AdminRequests.ProfileRequests.GetAllProfilesResponse
@{
    ViewData["Title"] = "Профили";
}

<div class="container-fluid px-4">
    <h1 class="mt-4">@ViewData["Title"]</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item"><a href="/admin">Панель управления</a></li>
        <li class="breadcrumb-item active">@ViewData["Title"]</li>
    </ol>
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            Список профилей
        </div>
        <div class="card-body">
            <table id="datatablesSimple">
                <thead>
                <tr>
                    <th>Id</th>
                    <th>User Id</th>
                    <th>Background Image</th>
                    <th>About Me</th>
                    <th>Working Zone</th>
                    <th>Education</th>
                    <th>Contacts</th>
                    <th>Facebook Link</th>
                    <th>Twitter Link</th>
                    <th>Google Link</th>
                    <th>Pinterest Link</th>
                    <th>Created Date</th>
                </tr>
                </thead>
                <tfoot>
                <tr>
                    <th>Id</th>
                    <th>User Id</th>
                    <th>Background Image</th>
                    <th>About Me</th>
                    <th>Working Zone</th>
                    <th>Education</th>
                    <th>Contacts</th>
                    <th>Facebook Link</th>
                    <th>Twitter Link</th>
                    <th>Google Link</th>
                    <th>Pinterest Link</th>
                    <th>Created Date</th>
                </tr>
                </tfoot>
                <tbody>
                @foreach (var profile in @Model.Profiles)
                {
                    <tr>
                        <td>@profile.Id</td>
                        <td>@profile.UserId</td>
                        <td>@profile.BackgroundImage</td>
                        <td>@profile.AboutMe</td>
                        <td>@profile.WorkingZone</td>
                        <td>@profile.Education</td>
                        <td>@profile.Contacts</td>
                        <td>@profile.FacebookLink</td>
                        <td>@profile.TwitterLink</td>
                        <td>@profile.GoogleLink</td>
                        <td>@profile.PinterestLink</td>
                        <td>@profile.CreatedDate</td>
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </div>
</div> 