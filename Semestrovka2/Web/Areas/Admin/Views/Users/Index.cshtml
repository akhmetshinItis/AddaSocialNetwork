@model Contracts.Requests.AdminRequests.UserRequests.GetAllUsersResponse
@{
    ViewData["Title"] = "Пользователи";
}

                    <div class="container-fluid px-4">
                        <h1 class="mt-4">Tables</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item"><a href="/admin">Dashboard</a></li>
                            <li class="breadcrumb-item active">Tables</li>
                        </ol>
                        
                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table me-1"></i>
                                Users
                            </div>
                            <div class="card-body">
                                <table id="datatablesSimple">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>First Name</th>
                                            <th>Last Name</th>
                                            <th>Gender</th>
                                            <th>Age</th>
                                            <th>Country</th>
                                            <th>Image URl</th>
                                            <th>Identity User ID</th>
                                            <th>Created Date</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>Id</th>
                                            <th>First Name</th>
                                            <th>Last Name</th>
                                            <th>Gender</th>
                                            <th>Age</th>
                                            <th>Country</th>
                                            <th>Image URl</th>
                                            <th>Identity User ID</th>
                                            <th>Created Date</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                    @foreach (var user in @Model.Users)
                                    {
                                        <tr>
                                            <td>@user.Id</td>
                                            <td>@user.FirstName</td>
                                            <td>@user.LastName</td>
                                            <td>@user.Gender</td>
                                            <td>@user.Age</td>
                                            <td>@user.Country</td>
                                            <td>@user.ImageUrl</td>
                                            <td>@user.IdentityUserId</td>
                                            <td>@user.CreatedDate</td>

                                        </tr>
                                    }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
